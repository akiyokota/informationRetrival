<script defer type="text/javascript">    
    function OnRadEditorLoad(editor, args)    
    {    
        editor.get_filtersManager().add(new MyFilter());    
        var images = editor.get_document().getElementsByTagName("IMG");    
        for (var i = 0; i < images.length; i++)    
         {    
            var image = images[i];                                
            if (image && image.getAttribute("NAME"))    
             {                                                    
                image.className = "articleImage";
            }    
          }    
    }    
    MyFilter = function()    
     {    
        MyFilter.initializeBase(this);    
        this.set_isDom(false);    
        this.set_enabled(true);    
        this.set_name("RadEditor filter");    
        this.set_description("RadEditor filter description");    
    }    
     MyFilter.prototype =    
     {    
        getHtmlContent : function(content)    
        {    
            var oDiv = document.createElement("DIV");    
            oDiv.innerHTML = content;    
            var images = oDiv.getElementsByTagName("IMG");    
            for (var i = 0; i < images.length; i++)    
             {    
                var image = images[i];                                
				if (image && image.getAttribute("NAME"))    
				{                                                    
					image.className = "";    
				}    
            }    
            return oDiv.innerHTML;    
        },    
  
        getDesignContent : function(content)    
         {    
            var oDiv = document.createElement("DIV");    
            oDiv.innerHTML = content;    
            var images = oDiv.getElementsByTagName("IMG");    
            for (var i = 0; i < images.length; i++)    
            {    
                var image = images[i];                                
				if (image && image.getAttribute("NAME"))    
				{                                                    
					image.className = "articleImage";
				}    
            }    
            return oDiv.innerHTML;    
        }    
    }    
    MyFilter.registerClass('MyFilter', Telerik.Web.UI.Editor.Filter);    
	
	Telerik.Web.UI.Editor.CommandList["InsertImage"] = function(commandName, editor, args)  
{  
    var elem = editor.getSelectedElement(); //returns the selected element.  
       if (elem.tagName == "IMG")  
       {  
            editor.selectElement(elem);  
            argument = elem;  
       }  
       else  
       {  
          var content = editor.getSelectionHtml();   
          var image = editor.get_document().createElement("IMG");  
          image.innerHTML = content;            
          argument = image;  
       }  
       var myCallbackFunction = function(sender, args)  
       {  
           editor.pasteHtml(args.imageName)
       }  
       editor.showExternalDialog(  
        '/DesktopModules/Admin/RadEditorProvider/Dialogs/InsertImage.aspx',  
        argument,  
        640,  
        580,  
        myCallbackFunction,  
         null,  
         'Insert Image',  
        true,  
        Telerik.Web.UI.WindowBehaviors.Close + Telerik.Web.UI.WindowBehaviors.Move,  
        false,  
        true);  
};  

</script> 

<script defer type="text/javascript">    
    function OnRadEditorLoad(editor, args)    
    {    
        editor.get_filtersManager().add(new MyFilter());    
        var links = editor.get_document().getElementsByTagName("DIV");    
        for (var i=0; i< links.length; i++)    
         {    
            var link = links[i];                                
            if (link && link.getAttribute("NAME"))    
             {                                                    
                link.className = "articleVideo";
            }    
          }    
    }    
    MyFilter = function()    
     {    
        MyFilter.initializeBase(this);    
        this.set_isDom(false);    
        this.set_enabled(true);    
        this.set_name("RadEditor filter");    
        this.set_description("RadEditor filter description");    
    }    
     MyFilter.prototype =    
     {    
        getHtmlContent : function(content)    
        {    
            var oDiv = document.createElement("DIV");    
            oDiv.innerHTML = content;    
            var links = oDiv.getElementsByTagName("A");    
            for (var i=0; i< links.length; i++)    
             {    
                 var link = links[i];                                
                    if (link && link.getAttribute("NAME"))    
                    {                                                    
                        link.className = "";    
                     }    
            }    
            return oDiv.innerHTML;    
        },    
  
        getDesignContent : function(content)    
         {    
            var oDiv = document.createElement("DIV");    
            oDiv.innerHTML = content;    
            var links = oDiv.getElementsByTagName("A");    
            for (var i=0; i< links.length; i++)    
            {    
                var link = links[i];                                
                    if (link && link.getAttribute("NAME"))    
                    {                                                    
                        link.className = "articleVideo";
                    }    
            }    
            return oDiv.innerHTML;    
        }    
    }    
    MyFilter.registerClass('MyFilter', Telerik.Web.UI.Editor.Filter);    
	
	Telerik.Web.UI.Editor.CommandList["InsertVideo"] = function(commandName, editor, args)  
{  
    var elem = editor.getSelectedElement(); //returns the selected element.  
       if (elem.tagName == "A")  
       {  
            editor.selectElement(elem);  
            argument = elem;  
       }  
       else  
       {  
          var content = editor.getSelectionHtml();   
          var link = editor.get_document().createElement("A");  
          link.innerHTML = content;            
          argument = link;  
       }  
       var myCallbackFunction = function(sender, args)  
       {  
           editor.pasteHtml(args.videoName)
       }  
       editor.showExternalDialog(  
        '/DesktopModules/Admin/RadEditorProvider/Dialogs/InsertVideo.aspx',  
        argument,  
        570,  
        400,  
        myCallbackFunction,  
         null,  
         'Insert Video',  
        true,  
        Telerik.Web.UI.WindowBehaviors.Close + Telerik.Web.UI.WindowBehaviors.Move,  
        false,  
        true);  
};  

</script>  


<!--End Script for Image UPload-->



