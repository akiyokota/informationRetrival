package CS172_Info_Ret.webCrawler.Objects;

public class CleanUrl {
	private String protocol;
	private String port ;
	private String host ;
	private String path ;
	private String query ;
	private String bookmark ;
	
	public CleanUrl() {
		this.protocol="";
		this.port="";
		this.host="";
		this.path="";
		this.query="";
		this.bookmark="";
	}
	
	/**
	 * Takes in whole url as a string, analyze and normalize it into cleanUrl object
	 * @param doc The document to be parsed
	 * @return List of URL's found
	 */
	public NormalizedUrl UrlNormalization (String url) {
		NormalizedUrl temp = null;
		if (url.equals("") || url==null) {
			System.err.println("Invalid url");
			return temp;
		}
		
		temp = new NormalizedUrl ();
		
		
		
		return temp;
	}
}
